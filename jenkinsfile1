

node{
agent any
stages {
        stage("Clone") {
            steps {
                git 'https://github.com/lamiaeveyrenc/projet-jenkins1.git'
            }
        }

        stage("Build") {
            steps {
                sh label: '', script: 'javac Main.java'
            }
        }
        stage("Run") {
            steps {
                sh label: '', script: 'java Main'
            }
        }
}
}
